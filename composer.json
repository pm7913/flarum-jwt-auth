    "authors": [
        {
            "name": "Stacey Friesen",
            "email": "staceyfriesen@gmail.com"
        }
    ],
    "require": {
        "flarum/core": "^0.1.0-beta.8",
        "lcobucci/jwt": "^3.2.5"
